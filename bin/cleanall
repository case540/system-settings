#!/bin/bash

wmctrl -l | grep -e "[^ ]*  0 " | awk '{print $1}' | xargs -I {} wmctrl -i -c {}
wmctrl -l | grep -e "[^ ]*  1 " | awk '{print $1}' | xargs -I {} wmctrl -i -c {}
wmctrl -l | grep -e "[^ ]*  2 " | grep -v "Google Chrome" | awk '{print $1}' | xargs -I {} wmctrl -i -c {}
wmctrl -l | grep -e "[^ ]*  3 " | awk '{print $1}' | xargs -I {} wmctrl -i -c {}
